// Online C compiler to run C program online
#include <stdio.h>

int main() {
    
    int num;
    

    do{
        printf("# MENU PRINCIPAL #\n [1]Inserir\n [2]Excluir\n [3]Consultar\n [4]Listar\n [5]Sair\n"); 
        scanf("%d", &num);
        
        if(num = 1){
         printf("Inserir\n\n"); 
        }
        else if(num = 2){
            printf("Excluir\n\n");
        }
        else if(num = 3){
            printf("Consultar\n\n");
        }
        else if(num = 4){
            printf("Listar\n\n");
        }
        
    }
    while(num != 5);
    printf("Sair");

    return 0;
}
